///Diseñar un programa que calule un aumento del 50% a un sueldo de 500

Algoritmo Aumentar
	
	//DECLARAR VARIABLES
	definir sueldo, descuento, aumento, porcentaje como real;
	Definir  stock como entero;
	
	//ASIGNO VALOR A UNA VARIABLE
	sueldo <- 500;
	porcentaje <- 50;
	
	//INCREMENTAR SUELDO 
	aumento <- porcentaje * sueldo / 100;
	Escribir sueldo + aumento ;
	
FinAlgoritmo
